@model Data3.Client

@{
    ViewBag.Title = "Details Client";
}

<h2>Details Client</h2>

<fieldset>
    <legend>Client</legend>

    <table class="table table-hover">
        <tr>
            <th>
                @Html.LabelForModel("Name")
            </th>
            <th>
                @Html.LabelForModel("Last Name 1")
            </th>
            <th>
                @Html.LabelForModel("Last Name 2")
            </th>
            <th>
